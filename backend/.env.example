
# Flask configuration
FLASK_APP=run.py
FLASK_ENV=development
SECRET_KEY=your_secret_key_here
JWT_SECRET_KEY=your_jwt_secret_key_here

# Database URLs
DEV_DATABASE_URL=postgresql://username:password@localhost/nexus_analytics_dev
TEST_DATABASE_URL=postgresql://username:password@localhost/nexus_analytics_test
DATABASE_URL=postgresql://username:password@localhost/nexus_analytics

# Redis configuration
REDIS_URL=redis://localhost:6379/0

# S3 configuration
S3_BUCKET=your_bucket_name
S3_KEY=your_aws_access_key
